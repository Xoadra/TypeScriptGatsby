{
	"name": "typescript-gatsby",
	"private": true,
	"description": "A simple starter to get up and developing quickly with Gatsby",
	"version": "0.1.0",
	"license": "MIT",
	"scripts": {
		"postinstall": "netlify-lambda install functions",
		"build": "npm run build:gatsby && npm run build:lambda",
		"build:gatsby": "gatsby build",
		"build:lambda": "netlify-lambda build functions",
		"start": "concurrently -p none -k \"npm:start:*\"",
		"start:develop": "gatsby develop",
		"start:function": "netlify-lambda serve functions -c webpack.functions.js",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
	},
	"dependencies": {
		"@types/aws-lambda": "^8.10.39",
		"@types/netlify-identity-widget": "^1.4.1",
		"@types/node": "^12.12.21",
		"@types/react": "^16.9.13",
		"@types/react-dom": "^16.9.4",
		"@types/react-helmet": "^5.0.14",
		"axios": "^0.19.0",
		"debug": "^4.1.1",
		"gatsby": "^2.18.11",
		"gatsby-image": "^2.2.33",
		"gatsby-plugin-manifest": "^2.2.29",
		"gatsby-plugin-netlify-cms": "^4.1.31",
		"gatsby-plugin-offline": "^3.0.23",
		"gatsby-plugin-react-helmet": "^3.1.15",
		"gatsby-plugin-sharp": "^2.3.3",
		"gatsby-plugin-typescript": "^2.1.20",
		"gatsby-source-filesystem": "^2.1.38",
		"gatsby-transformer-remark": "^2.6.38",
		"gatsby-transformer-sharp": "^2.3.9",
		"gray-matter": "^4.0.2",
		"hast-util-raw": "^5.0.1",
		"hast-util-to-html": "^6.0.2",
		"ink": "^2.6.0",
		"lodash": "^4.17.15",
		"mdast-util-to-hast": "^7.0.0",
		"mdast-util-to-string": "^1.0.7",
		"mdast-util-toc": "^5.0.0",
		"netlify-auth-providers": "^1.0.0-alpha5",
		"netlify-cms-app": "^2.9.7",
		"netlify-identity-widget": "^1.5.6",
		"netlify-lambda": "^1.6.3",
		"node-object-hash": "^2.0.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^5.2.1",
		"remark": "^11.0.2",
		"remark-parse": "^7.0.2",
		"remark-retext": "^3.1.3",
		"remark-stringify": "^7.0.4",
		"retext-english": "^3.0.4",
		"sanitize-html": "^1.20.1",
		"underscore.string": "^3.3.5",
		"unified": "^8.4.2",
		"unist-util-remove-position": "^2.0.0",
		"unist-util-select": "^3.0.0",
		"unist-util-visit": "^2.0.1",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-typescript": "^7.7.7",
		"babel-loader": "^8.0.6",
		"babel-preset-gatsby": "^0.2.26",
		"concurrently": "^5.0.2",
