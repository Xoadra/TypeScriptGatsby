{
	"compilerOptions": {
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"lib": [                             
			"dom",
			"esnext"
		],
		"module": "commonjs",
		"outDir": "./dist/",
		"sourceMap": true,
		"strict": true,
		"target": "esnext",
		"typeRoots": [
			"node_modules/@types"
		]
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"./node_modules/**/*",
		"./src/admin/*"
	]
}